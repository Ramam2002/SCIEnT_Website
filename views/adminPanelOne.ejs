<body>
    <div class="table-users" id="scheme1" >
        <div class="header">PROJECTS (SCHEME ONE)</div>
        <div class="scrollbar">
            <table  cellspacing="0">
                <tr>
                    <th width="25%">PROJECT ID</th>
                    <th width="45%">PROJECT NAME</th>
                    <th width="15%">APPROVE</th>
                    <th width="15%">REMOVE</th>
                </tr>
        		
                <% schemeOneRows.forEach(function(item) { %>
                <tr>
                    <td ><a href="#"><span class="projectId"><%= item.id %></span></a></td>
                    <td ><span class="projectName"><%= item.projectTitle %></span></td>
                    <td><button id="approveForProjectsByL1" type="button" class="btn btn-success">APPROVE </button></td>
                    <!-- <td><button type="button" class="btn btn-danger" style="font-size:70% ">REJECT  </button></td> -->
                    <td><span id="removeForProjects"><i style="color:red;cursor:pointer;" class="fa fa-close"></i></span>
                </tr>
                <% }); %>
            </table>  
        </div>
    </div>

    <!-- My first scheme is above-->
    
    <div class="table-users" id="scheme2" >
       <div class="header">FACILITIES/TOOLS (SCHEME TWO)</div>
       <div class="scrollbar">
       
           <table class="scrollbar" cellspacing="0">
                <tr>
                    <th width="25%">APPLICATION ID</th>
                    <th width="45%">APPLICANT ROLL</th>
                    <th width="15%">APPROVE</th>
                    <th width="15%">REMOVE</th>
                </tr>

                <% schemeTwoRows.forEach(function(item) { %>
                <tr>
                    <td ><a href="#"><span class="applicantId"><%= item.id %></span></a></td>
                    <td ><span class="applicantRoll"><%= item.roll %></span></td>
                    <td><button id="approveForFacilities" type="button" class="btn btn-success">APPROVE </button></td>
                    <!-- <td><button type="button" class="btn btn-danger" style="font-size:70% ">REJECT  </button></td> -->
                    <td><span id="removeForFacilities"><i style="color:red;cursor:pointer;" class="fa fa-close"></i></span>
                </tr>
                <% }); %>
               
           </table>  
        </div>
    </div>

<!-- my second scheme is above-->
    
    
