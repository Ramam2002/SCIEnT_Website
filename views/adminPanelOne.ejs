<!DOCTYPE html>
<html>
<head>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="/js/jquery.js"></script>
    <script src="/js/jquery-1.10.1.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/bootstrap.js"></script>
    <script src="/js/adminPanel.js"></script>
    <script src="/js/adminPanelDonations.js"></script>
    <script src="/js/adminPanelResources.js"></script>
    <script src="/js/adminPanelFacilities.js"></script>
    <script src="/js/adminPanelProjects.js"></script>
    <script src="/js/adminPanelAddProjects.js"></script>
    <script src="/js/adminPanelHallBooking.js"></script>
    <script src="/js/adminPanelInventory.js"></script>
    <script src="/js/adminPanelGallery.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/css?family=Vollkorn" rel="stylesheet">
    <link rel="stylesheet" href="/css/adminPanelOne.css">
    <link rel="stylesheet" href="/css/scheme1.css">
</head>
<style>

.table-responsive {
    max-height: 600px;
}

</style>
<body>

    <div class="container">
        <div class="row" style="margin-top:2%;">
            <div class="col-md-3">
                <p>Visitor Count = <%= visitorCount %></p>
            </div>
            <div class="col-md-6">
                <h1 style="font-family: 'Vollkorn', serif;text-align: center;">ADMIN PANEL</h1>
            </div>
            <div class="col-md-3">
                <form method="post" action="/admin/logout">
                    <button type="submit" class="btn btn-warning btn-lg">Logout</button>
                </form>
            </div>
        </div>


        <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#completedOrOngoingProjects">Completed/Ongoing Projects</a></li>
            <li><a data-toggle="tab" href="#pendingProjects">Pending Projects</a></li>
            <li><a data-toggle="tab" href="#facilitiesOrTools">Access</a></li>
            <li><a data-toggle="tab" href="#conferenceHallBooking">Hall Booking Requests</a></li>
            <li><a data-toggle="tab" href="#addProjects">Add Projects</a></li>
            <li><a data-toggle="tab" href="#donations">Donations</a></li>
            <li><a data-toggle="tab" href="#addAdmins">Add Admins</a></li>
            <li><a data-toggle="tab" href="#inventory">Inventory</a></li>
            <li><a data-toggle="tab" href="#resources">Resources</a></li>
            <li><a data-toggle="tab" href="#gallery">Gallery</a></li>
            <li><a data-toggle="tab" href="#changePasswords">Change your password</a></li>
        </ul>

        <div class="tab-content">
            <div id="completedOrOngoingProjects" class="tab-pane fade in active">
                <div class="row" style="margin-top:3%;">
                    <div class="col-md-12">              
                        <div class="table-responsive" id="projectsTable">          
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th class="header"></th>
                                        <th class="header">Completed or ongoing Projects</th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <!-- <th class="header"><button type="button" id="mailForProjects" class="btn btn-success">Mail for Projects</button></th> -->
                                    </tr>
                                </thead>

                                <thead>
                                    <tr>
                                        <th class="subheader">PROJECT ID</th>
                                        <th class="subheader">PROJECT NAME</th>
                                        <th class="subheader">MARK AS COMPLETE</th>
                                        <th class="subheader">STATUS</th>
                                        <th class="subheader">LATEST UPDATE DONE BY</th>
                                     </tr>
                                </thead>
                                <tbody>
                                    <% projectsRows.forEach(function(item) { %>
                                        <% if (item.status == 'Completed' || item.status == 'Ongoing') { %>
                                            <tr>
                                                <td ><a href="#"><span class="projectId"><%= item.id %></span></a></td>
                                                <td ><span class="projectName"><%= item.projectTitle %></span></td>
                                                <% if (item.status == 'Completed') { %>
                                                    <td><button type="button" class="markAsComplete btn btn-success" disabled >MARK</button></td>
                                                <% } else { %> 
                                                    <td><button type="button" class="markAsComplete btn btn-success">MARK</button></td>
                                                <% } %>
                                                <td><span id="status"><%= item.status %></span></td>
                                                <td><span id="lastUpdater" class="lastUpdater"><%= item.latestUpdater %></span></td>
                                            </tr>
                                        <% } %>
                                    <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div id="pendingProjects" class="tab-pane fade">
                <div class="row" style="margin-top: 3%;">
                    <div class="col-md-12">
                        <div class="table-responsive" id="projectsApprovedByL2">
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header">PENDING PROJECTS</th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                    </tr>
                                </thead>
                                <thead>
                                    <tr>
                                        <th>PROJECT ID</th>
                                        <th>PROJECT NAME</th>
                                        <th>APPROVE</th>
                                        <th>REJECT</th>
                                        <th>BEGIN PROJECT</th>
                                        <th>STATUS</th>
                                        <th>LAST UPDATE DONE BY</th>
                                        <th>ENTER REMARKS</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% projectsRows.forEach(function(item) { %>
                                        <% if (item.status != 'Completed' &&item.status != 'Ongoing') { %>
                                            <tr>
                                                <td ><a href="#"><span class="projectId"><%= item.id %></span></a></td>
                                                <td ><span class="projectName"><%= item.projectTitle %></span></td>
                                                <% if (item.status == 'Approved by L1' || item.status == 'Rejected by L2' || item.status == 'Approved by L2') { %>
                                                    <td><button type="button" class="approveForProjectsByL1 btn btn-success" disabled>APPROVE</button></td>
                                                <% } else { %>
                                                    <td><button type="button" class="approveForProjectsByL1 btn btn-success">APPROVE</button></td>
                                                <% } %>
                                                <% if (item.status == 'Rejected by L1' || item.status == 'Rejected by L2' || item.status == 'Approved by L2') { %>
                                                    <td><button type="button"  class="rejectForProjectsByL1 btn btn-danger" disabled>REJECT</button></td>
                                                <% } else { %>
                                                    <td><button type="button" class="
                                                    rejectForProjectsByL1 btn btn-danger">REJECT</button></td>
                                                <% } %>
                                                 <% if (item.status == 'Approved by L2') { %>
                                                    <td><button type="button" class="beginProject btn btn-success">BEGIN</button></td>
                                                <% } else { %> 
                                                    <td><button type="button" class="beginProject btn btn-success" disabled>BEGIN</button></td>
                                                <% } %>
                                                <td><span id="status"><%= item.status %></span></td>
                                                <td><span class="lastUpdater"><%= item.latestUpdater %></span></td>
                                                <td><button type="button" class="enterRemarksByL1 btn btn-warning">REMARK</button></td>
                                            </tr>
                                        <% } %>
                                    <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div id="facilitiesOrTools" class="tab-pane fade">
                <div class="row" style="margin-top: 3%;">  
                    <div class="col-md-12">                
                        <div class="table-responsive" id="facilitiesTable">          
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th class="header"></th>
                                        <th class="header">ACCESS</th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"><button type="button" id="mailForFacilities" class="btn btn-success">Mail for Access</button></th>
                                        <th class="header"><button type="button" id="exportFacilitiesCSV" class="btn btn-success">Export CSV</button></th>
                                  </tr>
                                </thead>
             
                                <thead>
                                     <tr>
                                        <th>APPLICANT ID</th>
                                        <th>APPLICANT ROLL</th>
                                        <th>APPLICANT NAME</th>
                                        <th>PURPOSE</th>
                                        <th>DURATION</th>
                                        <th>APPROVE</th>
                                        <th>CURRENT STATUS</th>
                                        <th>EDIT STATUS</th>  
                                    </tr>
                                </thead>
                                <tbody>
                                    <% facilitiesRows.forEach(function(item) { %>
                                        <% if (item.status == 'Access given') { %>
                                        <tr style="background-color: #90EE90; ">
                                        <% } else if (item.status == 'Access not given') { %>
                                        <tr style="background-color: #FFA07A; ">
                                        <% } else { %> 
                                        <tr>
                                        <% } %>   
                                            <td ><a href="#"><span class="applicantId"><%= item.id %></span></a></td>
                                            <td ><span class="applicantRoll"><%= item.roll %></span></td>
                                            <td ><span class="applicantRoll"><%= item.name %></span></td>
                                            <td ><span class="applicantRoll"><%= item.purpose %></span></td>
                                            <td ><span class="applicantRoll"><%= item.duration %></span></td>
                                            <% if (item.approved == 'Yes') { %>
                                                <td><button type="button" class="approveForFacilities btn btn-success" disabled>APPROVE </button></td>
                                            <% } else { %> 
                                                <td><button type="button" class="approveForFacilities btn btn-success">APPROVE </button></td>
                                            <% } %>
                                            <td ><span class="applicantStatus"><%= item.status %></span></td>
                                            <td><span class="editAccess"><i style="color:#666;cursor:pointer;" class="fa fa-edit"></i></span></td>
                                            <!-- <td><span class="removeForFacilities"><i style="color:red;cursor:pointer;" class="fa fa-close"></i></span></td> -->
                                        </tr>
                                    <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div id="conferenceHallBooking" class="tab-pane fade">
                <div class="row" style="margin-top: 3%;">
                    <div class="col-md-12">                 
                        <div class="table-responsive" id="hallBookingRequests">
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header">HALL BOOKING</th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"><button type="button" class="mailForHallBooking btn btn-success">Mail for Hall Booking</button></th>
                                    </tr>
                                </thead>
                                <thead>
                                    <tr>
                                        <th>BOOKING ID</th>
                                        <th>PERSON ROLL</th>
                                        <th>NAME</th>
                                        <th>DATE</th>
                                        <th>START TIME</th>
                                        <th>END TIME</th>
                                        <th>APPROVE</th>
                                        <th>REMOVE</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% hallBookingRows.forEach(function(item) { %>
                                        <tr>
                                            <td ><a href="#"><span class="bookingId"><%= item.id %></span></a></td>
                                            <td ><span class="applicantRoll"><%= item.roll %></span></td>
                                            <td ><span class="applicantName"><%= item.name %></span></td>
                                            <td ><span class="applicantDate"><%= item.date.getDate() 
                                            + '/'
                                            + parseInt(item.date.getMonth() + 1)
                                            + '/'
                                            + item.date.getFullYear() %></span></td>
                                            <td ><span class="applicantRequestedStartTime"><%= item.startTime %></span></td>
                                            <td ><span class="applicantRequestedEndTime"><%= item.endTime %></span></td>
                                            <% if (item.approved == 'Yes') { %>
                                                <td><button type="button" class="approveForHallBooking btn btn-success" disabled>APPROVE </button></td>
                                            <% } else { %> 
                                                <td><button type="button" class="approveForHallBooking btn btn-success">APPROVE </button></td>
                                            <% } %>
                                            <td><span class="removeForHallBooking"><i style="color:red;cursor:pointer;" class="fa fa-close"></i></span></td>
                                        </tr>
                                    <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>


            <div id="addProjects" class="tab-pane fade">
                <div class="row" style="margin-top: 4%">
                    <div class="col-md-3" id="addProjectForm">
                        <h2 style="font-family: 'Vollkorn',serif;">ADD PROJECTS</h2>
                        <label>Project Title: </label><br>
                        <input type="text" id="projectName" name="projectName" autocomplete="off" required><br>
                        <label>Enter project description: </label><br>
                        <textarea name="projectDesc" id="projectDesc" cols="24" rows="5" autocomplete="off" required></textarea><br>
                        <label>Add project image : </label></br>
                        <input type="file" name = "projectImage" id ="projectImage"></br>
                        <span id="addProjectFormMsg" ></span></br>
                        <button id="addProject" class="btn btn-warning btn-lg">Add Project</button>  
                    </div>
                    <div class="col-md-1"></div>
                    <div class="col-md-8">
                        <div class="projectList">
                            <div class="table-responsive" id="projectListDisplay">
                                <table class="table  table-bordered">
                                    <thead>
                                       <tr>
                                        <th>PROJECT ID</th>
                                        <th>PROJECT IMAGE</th>
                                        <th>PROJECT NAME</th>
                                        <th>DESCRIPTION</th>
                                       </tr>
                                    </thead>
                                    <tbody>
                                        <% adminProjectRows.forEach(function(item){  %>
                                            <tr>
                                                <td><span class="projectsID"><%= item.id %></span></td>
                                                <td><span class="projectImage"><img width="400" height="225" src="<%= item.projectImage %>" ></span></td>
                                                <td><span class="projectsTitle"><%= item.projectTitle %></span></td>
                                                <td><span class="projectsDesc"><%= item.projectDesc %></span></td>
                                            </tr>
                                        <% }); %>
                                    </tbody>
                                 </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


            <div id="donations" class="tab-pane fade">
                <div class="row" style="margin-top: 3%;">
                    <div class="col-md-12">                 
                        <div class="table-responsive" id="donationsTable">
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th class="header"></th>                                        
                                        <th class="header"></th>
                                        <th class="header">DONATIONS</th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                    </tr>
                                </thead>
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>NAME</th>
                                        <th>PURPOSE/REMARKS</th>
                                        <th>AMOUNT</th>
                                        <th>PAID</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% donationRows.forEach(function(item) { %>
                                        <tr>
                                            <td><a href="#"><span class="donationId"><%= item.id %></span></a></td>
                                            <td><span class="donorName"><%= item.name %></span></td>
                                            <td><span class="donationPurpose"><%= item.remarks %></span></td>
                                            <td><span class="donationAmount"><%= item.amount %></span></td>
                                            <td><span class="paid"><%= item.paid %></span></td>
                                        </tr>
                                    <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

            <div id="addAdmins" class="tab-pane fade">
                <div class="row" style="margin-top: 3%;">
                    <div class="col-md-4" id="addAdminForm">
                        <h2 style="font-family: 'Vollkorn',serif;">ADD ADMINS</h2>
                        <label>Enter username for admin:</label><br>
                        <input type="text" id="userName" name="userName" autocomplete="off"><br>
                        <label>Enter password for admin:</label><br>
                        <input type="password" id="adminPassword" name="adminPassword" autocomplete="off"><br>
                        <label>Confirm password by re-entering:</label><br>
                        <input type="password" id="confirmPassword" name="confirmPassword" autocomplete="off"><br>
                        <label>Select the level of admin:</label><br>
                        <select id="adminLevel" name="adminLevel">
                            <option value="One">One</option>
                            <option value="Two">Two</option>
                        </select><br><br>
                        <span id="addAdminMsg"></span>
                        <button id="addAdmin" class="btn btn-warning btn-lg">Add Admin</button>
                    </div>
                    <div class="col-md-2"></div>
                    <div class="col-md-6">
                        <div class="panel panel-info">
                            <div class="panel-heading">Present Scient Admins</div>
                            <div class="panel-body">
                                <h6>Level one admins</h6>
                                <ul id="levelOneAdminsList">
                                <% adminsRows.forEach(function(item) { %>
                                    <% if ( item.adminLevel == 'One') { %>
                                        <li><%= item.adminName %></li>
                                    <% } %>
                                <% }); %>
                                </ul>
                                <h6>Level two admins</h6>
                                <ul id="levelTwoAdminsList">
                                <% adminsRows.forEach(function(item) { %>
                                    <% if ( item.adminLevel == 'Two') { %>
                                        <li><%= item.adminName %></li>
                                    <% } %>
                                <% }); %>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            
            <div id="inventory" class="tab-pane fade">
                <div class="row" style="margin-top:3%;">
                    <div class="col-md-12">              
                        <div class="table-responsive" id="inventoryTable">          
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header">Inventory</th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"></th>
                                        <th class="header"><button type="button" id="addNewVendor" class="btn btn-success">Create Vendor</button></th>
                                        <th class="header"><button type="button" id="addNewProduct" class="btn btn-success">Add New Product</button></th>
                                        <!-- <th class="header"><button type="button" id="mailForProjects" class="btn btn-success">Mail for Projects</button></th> -->
                                    </tr>
                                </thead>

                                <thead>
                                    <tr>
                                        <th class="subheader">SL.NO</th>
                                        <th class="subheader">PRODUCT CODE</th>
                                        <th class="subheader">DESCRIPTION</th>
                                        <th class="subheader">UNIT</th>
                                        <th class="subheader">QUANTITY</th>
                                        <th class="subheader">PRICE</th>
                                        <th class="subheader">VENDOR</th>
                                        <th class="subheader">BILLING DETAILS</th>
                                        <th class="subheader">REMARKS</th>
                                        <th class="subheader">EDIT PRODUCT</th>
                                        <th class="subheader">REMOVE</th>
                                     </tr>
                                </thead>
                                <tbody>
                                   <% inventoryRows.forEach(function(item) { %>
                                                <tr>
                                                    <td ><a href="#"><span class="inventoryId"><%= item.id %></span></a></td>            
                                                    <td ><span class="productCode"><%= item.productCode %></span></td>
                                                    <td ><span class="descriptionInventory"><%= item.descriptionInventory %></span></td>
                                                    <td ><span class="unit"><%= item.unit %></span></td>
                                                    <td ><span class="quantity"><%= item.quantity %></span></td>
                                                    <td ><span class="price"><%= item.price %></span></td>
                                                    <td ><a href="#"><span class="vendorName"><%= item.vendorName %></span></a></td>
                                                    <td ><a href="#"><span class="billNumber"><%= item.billNumber %></span></a></td>
                                                    <td ><span class="productRemarks"><%= item.remarks %></span></td>
                                                    <td><span class="editForInventoryProducts"><i style="color:#777;cursor:pointer;" class="fa fa-edit"></i></span></td>
                                                    <td><span class="removeForInventoryProducts"><i style="color:red;cursor:pointer;" class="fa fa-close"></i></span></td>
                                                </tr>
                                            <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

            
            <div id="resources" class="tab-pane fade">
                <div class="row" style="margin-top: 3%;">
                    <div class="col-md-4" id="addResourcesForm">
                        <h2 style="font-family: 'Vollkorn',serif;">ADD RESOURCE</h2>
                        
                        <form method="POST" enctype="multipart/form-data" name="resourceForm" id="resourceForm">
                            <div class="input-field col s6">
                                <label for="name">Resource Name</label><br>
                                <input id="name" name="name" type="text" class="validate">
                            </div>
                            <br>
                            <div class="input-field col s6">
                                <label for="type">Resource Type</label><br>
                                <p>
                                    <span>Electronics</span>
                                    <input name="type" value="electronics" type="radio"/>
                                </p>
                                <p>
                                    <span>Mechanical</span>
                                    <input name="type" value="mechanical" type="radio"/>
                                </p>
                                <p>
                                    <span>Others</span>
                                    <input name="type" value="others" type="radio" checked/>
                                </p>
                            </div>
                            <br>
                            <div class="file-field input-field">
                                    <label for="resourceImage">Image</label><br>
                                    <input id="resourceImage" name="resourceImage" type="file">
                            </div>
                            <br>
                            <span id="resourceMsg"></span>
                            <br>
                            <button type="submit" class="btn btn-warning btn-lg">Submit Resource</button>
                        </form>
                    </div>
                </div>
            </div>

            <div id="gallery" class="tab-pane fade">
                <div class="row" style="margin-top: 3%;">
                    <div class="col-md-4" id="addGalleryImageForm">
                        <h2 style="font-family: 'Vollkorn',serif;">ADD GALLERY IMAGE</h2>                        
                        <form method="POST" enctype="multipart/form-data" name="galleryImageForm" id="galleryImageForm">
                            <div class="input-field col s6">
                                <label for="name">Image Title</label><br>
                                <input id="name" name="name" type="text" class="validate">
                            </div>
                            <br>
                            <div class="file-field input-field">
                                    <label for="galleryImage">Image</label><br>
                                    <input id="galleryImage" name="galleryImage" type="file">
                            </div>
                            <br>
                            <span id="galleryMsg"></span>
                            <br>
                            <button type="submit" class="btn btn-warning btn-lg">Submit Gallery Image</button>
                        </form>
                    </div>
                </div>
            </div>


            <div id="changePasswords" class="tab-pane fade">
                <div class="row" style="margin-top: 3%;">
                    <div class="col-md-4" id="changePasswordForm">
                        <h2 style="font-family: 'Vollkorn',serif;">CHANGE YOUR PASSWORD</h2>
                        <label>Enter your current password:</label><br>
                        <input type="password" id="currentPassword" name="currentPassword" autocomplete="off"><br>
                        <label>Enter new password:</label><br>
                        <input type="password" id="newPassword" name="newPassword" autocomplete="off"><br>
                        <label>Re-enter the new password to confirm:</label><br>
                        <input type="password" id="confirmPassword1" name="confirmPassword1" autocomplete="off"><br>
                        <span id="passwordChangeMsg"></span>
                        <br>
                        <button id="changePassword" class="btn btn-warning btn-lg">Change Password</button>
                    </div>
                </div>
            </div>
                    
        </div>
    </div>
     

