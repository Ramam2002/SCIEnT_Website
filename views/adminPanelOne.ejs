    <div class="table-users" id="scheme4" >
       <div class="header">PROJECTS APPROVED BY ALUMNI</div>
       <div class="scrollbar">
           <table  class="scrollbar"cellspacing="0">
                <tr>
                    <th width="25%">PROJECT ID</th>
                    <th width="60%">PROJECT NAME</th>
                    <th width="15%">REMOVE</th>
                </tr>   
                <% schemeOneRows.forEach(function(item) { %>
